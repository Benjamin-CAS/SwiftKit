<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--searchmk-->
    <attr name="SearchMKLayout_SearchMKLayoutStyle" format="reference" />
    <declare-styleable name="SearchMKLayout">
        <attr name="searchMKLayout_search_icon" format="string|reference" />
        <attr name="searchMKLayout_search_iconSize" format="dimension" />
        <attr name="searchMKLayout_search_iconPadding" format="dimension" />
        <attr name="searchMKLayout_search_background" format="reference" />
        <attr name="searchMKLayout_search_textSize" format="dimension" />
        <attr name="searchMKLayout_search_textColor" format="color" />

        <attr name="searchMKLayout_clear_icon" format="string" />
        <attr name="searchMKLayout_clear_iconSize" format="dimension" />

        <attr name="searchMKLayout_hint_text" format="string" />
        <attr name="searchMKLayout_hint_textSize" format="dimension" />
        <attr name="searchMKLayout_hint_textColor" format="color" />
        <attr name="searchMKLayout_hint_gravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
        </attr>

        <attr name="searchMKLayout_keyword_icon" format="string" />
        <attr name="searchMKLayout_keyword_iconSize" format="dimension" />
        <attr name="searchMKLayout_keyword_iconColor" format="color" />
        <attr name="searchMKLayout_keyword_background" format="reference" />
        <attr name="searchMKLayout_keyword_maxLength" format="integer" />
        <attr name="searchMKLayout_keyword_padding" format="dimension" />
    </declare-styleable>

    <!--navmk-->
    <attr name="NavMKBar_NavMKBarStyle" format="reference" />
    <declare-styleable name="NavMKBar">
        <attr name="navMKBar_title" format="string" />
        <attr name="navMKBar_title_textColor" format="color" />
        <attr name="navMKBar_title_textSize" format="dimension" />
        <attr name="navMKBar_title_textSize_with_subTitle" format="dimension" />

        <attr name="navMKBar_subTitle" format="string" />
        <attr name="navMKBar_subTitle_textSize" format="dimension" />
        <attr name="navMKBar_subTitle_textColor" format="color" />

        <attr name="navMKBar_textBtn_textSize" format="dimension" />
        <attr name="navMKBar_textBtn_textColor" format="color" />

        <attr name="navMKBar_icon_size" format="dimension" />
        <attr name="navMKBar_icon_color" format="color" />
        <attr name="navMKBar_icon" format="string" />

        <attr name="navMKBar_lineColor" format="color" />
        <attr name="navMKBar_lineWidth" format="dimension" />

        <attr name="navMKBar_paddingHorizontal" format="dimension" />
    </declare-styleable>

    <!--bannermk-->
    <declare-styleable name="BannerMK">
        <attr name="bannerMK_autoPlay" format="boolean" />
        <attr name="bannerMK_loop" format="boolean" />
        <attr name="bannerMK_intervalTime" format="integer|reference" />
    </declare-styleable>

    <!--btnmk-->
    <declare-styleable name="SwitchApple">
        <!--默认开关状态-->
        <attr name="switchApple_defaultStatus" format="boolean" />
        <!--打开时颜色-->
        <attr name="switchApple_bgColorOn" format="reference|color" />
        <!--关闭时颜色-->
        <attr name="switchApple_bgColorOff" format="reference|color" />
        <!--外边框颜色-->
        <attr name="switchApple_bgColorBorder" format="reference|color" />
        <!--中心圆颜色-->
        <attr name="switchApple_iconColorBtn" format="reference|color" />
        <!--外边框厚度-->
        <attr name="switchApple_borderWidth" format="dimension" />
        <!--变换时间-->
        <attr name="switchApple_animTime" format="integer" />
        <!--中心圆到外边距的距离-->
        <attr name="switchApple_iconMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BtnMKIcon">
        <attr name="btnMKIcon_background" format="reference" />
        <attr name="btnMKIcon_iconSrc" format="reference" />
        <attr name="btnMKIcon_iconSize" format="dimension|reference" />
        <attr name="btnMKIcon_iconMarginRight" format="dimension|reference" />
        <attr name="btnMKIcon_text" format="string|reference" />
        <attr name="btnMKIcon_textColor" format="color|reference" />
        <attr name="btnMKIcon_textSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="BtnMKSpinnerForm">
        <attr name="btnMKSpinnerForm_isRequired" format="boolean" />
        <attr name="btnMKSpinnerForm_requiredIconPos" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="btnMKSpinnerForm_label" format="string|reference" />
        <attr name="btnMKSpinnerForm_labelTextSize" format="dimension|reference" />
        <attr name="btnMKSpinnerForm_labelColor" format="color|reference" />
        <attr name="btnMKSpinnerForm_labelMarginLeft" format="dimension|reference" />
        <attr name="btnMKSpinnerForm_labelMarginRight" format="dimension|reference" />
        <attr name="btnMKSpinnerForm_labelWidth" format="dimension|reference" />
        <!--StringArray 字符串数组-->
        <attr name="btnMKSpinnerForm_spinnerEntries" format="reference" />
        <!--layout item布局-->
        <attr name="btnMKSpinnerForm_spinnerItemLayout" format="reference" />
        <attr name="btnMKSpinnerForm_borderBackground" format="reference" />
    </declare-styleable>

    <!--layoutmk-->
    <declare-styleable name="LayoutMKAmount">
        <attr name="layoutMKAmount_btn_textSize" format="dimension|reference" />
        <attr name="layoutMKAmount_btn_textColor" format="color|reference" />
        <attr name="layoutMKAmount_btn_size" format="dimension|reference" />
        <attr name="layoutMKAmount_btn_background" format="color" />

        <attr name="layoutMKAmount_margin" format="dimension|reference" />

        <attr name="layoutMKAmount_num_textSize" format="dimension" />
        <attr name="layoutMKAmount_num_textColor" format="color" />
        <attr name="layoutMKAmount_num_size" format="dimension" />
        <attr name="layoutMKAmount_num_background" format="color" />
        <attr name="layoutMKAmount_num_defaultValue" format="integer" />
        <attr name="layoutMKAmount_num_maxValue" format="integer" />
        <attr name="layoutMKAmount_num_minValue" format="integer" />
    </declare-styleable>

    <!--视频布局-->
    <declare-styleable name="LayoutMKVideo">
        <!--视频地址-->
        <attr name="layoutMKVideo_pathOrUrl" format="string" />
        <!--视频是否循环播放-->
        <attr name="layoutMKVideo_isLoop" format="boolean" />
        <!--视频权重-->
        <attr name="layoutMKVideo_videoGravity" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
            <enum name="centerCrop" value="2" />
            <enum name="none" value="3" />
        </attr>
    </declare-styleable>

    <!--毛玻璃布局-->
    <declare-styleable name="LayoutMKBlurred">
        <!--图片src-->
        <attr name="layoutMKBlurred_src" format="reference" />
        <!--是否图片移动-->
        <attr name="layoutMKBlurred_isMove" format="boolean" />
        <!--是否禁用模糊-->
        <attr name="layoutMKBlurred_isDisableBlurred" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LayoutMKInputItem">
        <attr name="layoutMKInputItem_hint" format="string" />
        <attr name="layoutMKInputItem_title" format="string" />
        <attr name="layoutMKInputItem_inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="number" value="2" />
        </attr>
        <attr name="layoutMKInputItem_inputTextAppearance" format="reference" />
        <attr name="layoutMKInputItem_titleTextAppearance" format="reference" />
        <attr name="layoutMKInputItem_topLineAppearance" format="reference" />
        <attr name="layoutMKInputItem_bottomLineAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="LayoutMKInputItemInputTextAppearance">
        <attr name="layoutMKInputItem_hintColor" format="reference|color" />
        <attr name="layoutMKInputItem_inputColor" format="color|reference" />
        <attr name="layoutMKInputItem_textSize" format="dimension" />
        <attr name="layoutMKInputItem_maxInputLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="LayoutMKInputItemTitleTextAppearance">
        <attr name="layoutMKInputItem_titleColor" format="reference|color" />
        <attr name="layoutMKInputItem_titleSize" format="color|reference" />
        <attr name="layoutMKInputItem_minWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LayoutMKInputItemLineAppearance">
        <attr name="layoutMKInputItem_color" format="reference|color" />
        <attr name="layoutMKInputItem_height" format="dimension" />
        <attr name="layoutMKInputItem_leftMargin" format="dimension" />
        <attr name="layoutMKInputItem_rightMargin" format="dimension" />
        <attr name="layoutMKInputItem_enable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SliderMKLayout">
        <attr name="sliderMKLayout_menuItemWidth" format="dimension" />
        <attr name="sliderMKLayout_menuItemHeight" format="dimension" />
        <attr name="sliderMKLayout_menuItemTextSize" format="dimension" />
        <attr name="sliderMKLayout_menuItemSelectTextSize" format="dimension" />
        <attr name="sliderMKLayout_menuItemIndicator" format="reference" />
        <attr name="sliderMKLayout_menuItemTextColor" format="reference" />
        <attr name="sliderMKLayout_menuItemBackgroundColor" format="reference|color" />
        <attr name="sliderMKLayout_menuItemSelectBackgroundColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="CircleSpread">
        <!--中心圆颜色-->
        <attr name="circleSpread_centerColor" format="reference|color" />
        <!--中心圆图片-->
        <attr name="circleSpread_centerDrawable" format="reference" />
        <!--中心圆半径-->
        <attr name="circleSpread_centerRadius" format="dimension" />
        <!--扩散圆颜色-->
        <attr name="circleSpread_spreadColor" format="reference|color" />
        <!--扩散圆最大数-->
        <attr name="circleSpread_spreadCount" format="integer" />
        <!--扩散圆波浪叠层效果还是边缘波纹效果-->
        <attr name="circleSpread_spreadIsRipple" format="boolean" />
        <!--扩散间距-->
        <attr name="circleSpread_circlesDistance" format="dimension" />
        <!--扩散时间1-1000ms之间-->
        <attr name="circleSpread_animTime" format="integer" />
    </declare-styleable>

    <!--textmk-->
    <!--下划线控件-->
    <declare-styleable name="TextMKUnderLine">
        <!--下划线是否出现-->
        <attr name="textMKUnderLine_underline" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextMKTextForm">
        <attr name="textMKTextForm_isRequired" format="boolean" />
        <attr name="textMKTextForm_requiredIconPos" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="textMKTextForm_label" format="string|reference" />
        <attr name="textMKTextForm_labelTextSize" format="dimension|reference" />
        <attr name="textMKTextForm_labelColor" format="color|reference" />
        <attr name="textMKTextForm_labelMarginLeft" format="dimension|reference" />
        <attr name="textMKTextForm_labelMarginRight" format="dimension|reference" />
        <attr name="textMKTextForm_labelWidth" format="dimension|reference" />
        <attr name="textMKTextForm_textColor" format="color|reference" />
        <attr name="textMKTextForm_textHint" format="string|reference" />
        <attr name="textMKTextForm_textSize" format="dimension|reference" />
        <attr name="textMKTextForm_borderBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="TextMKEditForm">
        <attr name="textMKEditForm_isRequired" format="boolean" />
        <attr name="textMKEditForm_requiredIconPos" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="textMKEditForm_label" format="string|reference" />
        <attr name="textMKEditForm_labelTextSize" format="dimension|reference" />
        <attr name="textMKEditForm_labelColor" format="color|reference" />
        <attr name="textMKEditForm_labelMarginLeft" format="dimension|reference" />
        <attr name="textMKEditForm_labelMarginRight" format="dimension|reference" />
        <attr name="textMKEditForm_labelWidth" format="dimension|reference" />
        <attr name="textMKEditForm_editHint" format="string|reference" />
        <attr name="textMKEditForm_editType" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="textPassword" value="2" />
            <enum name="numberPassword" value="3" />
        </attr>
        <attr name="textMKEditForm_editColor" format="color|reference" />
        <attr name="textMKEditForm_editSize" format="dimension|reference" />
        <attr name="textMKEditForm_borderBackground" format="reference" />
        <attr name="textMKEditForm_borderBackgroundHasFocus" format="reference" />
    </declare-styleable>

    <!--viewmk-->
    <declare-styleable name="SquareQRScan">
        <!-- 扫描边框的粗细-->
        <attr name="squareQRScan_borderWidth" format="dimension" />
        <!-- 扫描边框的颜色-->
        <attr name="squareQRScan_borderColor" format="color|reference" />
        <!-- 扫描线的图片资源-->
        <attr name="squareQRScan_lineDrawable" format="reference" />
        <!-- 扫描线的粗细-->
        <attr name="squareQRScan_lineWidth" format="dimension" />
        <!-- 扫描线的颜色-->
        <attr name="squareQRScan_lineColor" format="color|reference" />
        <!-- 扫描线每次移动的距离(包括扫描线的移动,收缩和成功logo的展现)-->
        <attr name="squareQRScan_distance" format="dimension" />
        <!-- 扫描线从顶部移动至底部的动画时间ms-->
        <attr name="squareQRScan_animTime" format="integer" />
        <!-- 扫描线是否来回移动-->
        <attr name="squareQRScan_isLineReverse" format="boolean" />
        <!-- 扫描成功的成功图标-->
        <attr name="squareQRScan_successDrawable" format="reference" />
        <!-- 扫描成功的成功图标大小-->
        <attr name="squareQRScan_successDrawableSize" format="dimension" />
        <!-- 扫描成功的成功动画时间-->
        <attr name="squareQRScan_successAnimTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="ArcRotate">
        <!-- 圆弧的开始渐变色-->
        <attr name="arcRotate_gradientStart" format="color|reference" />
        <!-- 圆弧的结束渐变色-->
        <attr name="arcRotate_gradientEnd" format="color|reference" />
        <!-- 圆弧的线宽-->
        <attr name="arcRotate_lineWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RadarRipple">
        <!-- 扫描雷达背景颜色-->
        <attr name="radarRipple_bgColor" format="color|reference" />
        <!-- 扫描雷达颜色-->
        <attr name="radarRipple_radarColor" format="reference|color" />
        <!-- 扫描雷达线颜色-->
        <attr name="radarRipple_lineColor" format="reference|color" />
        <!-- 扫描雷达线的Alpha-->
        <attr name="radarRipple_lineAlpha" format="integer" />
        <!-- 扫描雷达线宽-->
        <attr name="radarRipple_lineWidth" format="dimension" />
        <!-- 扫描雷达线数-->
        <attr name="radarRipple_lineCount" format="integer" />
        <!-- 扫描雷达扫描角度步长(1-360)-->
        <attr name="radarRipple_angleStep" format="integer" />
        <!-- 扫描雷达扫描一圈的时间ms-->
        <attr name="radarRipple_animTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="RadarWave">
        <!-- 扫描雷达颜色-->
        <attr name="radarWave_radarColor" format="reference|color" />
        <!-- 扫描雷达波浪颜色-->
        <attr name="radarWave_waveColor" format="reference|color" />
        <!-- 扫描雷达波浪层级渐变反转-->
        <attr name="radarWave_isWaveOverTurn" format="boolean" />
        <!-- 扫描雷达波浪层数-->
        <attr name="radarWave_waveCount" format="integer" />
        <!-- 扫描雷达扫描角度步长(1-360)-->
        <attr name="radarWave_angleStep" format="integer" />
        <!-- 扫描雷达扫描一圈的时间ms-->
        <attr name="radarWave_animTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="RingRotate">
        <!-- 旋转圆环开始颜色-->
        <attr name="ringRotate_ringStartColor" format="reference|color" />
        <!-- 旋转圆环结束颜色-->
        <attr name="ringRotate_ringEndColor" format="reference|color" />
        <!-- 旋转圆环宽度-->
        <attr name="ringRotate_ringWidth" format="dimension" />
        <!-- 旋转圆环步长-->
        <attr name="ringRotate_angleStep" format="integer" />
        <!-- 旋转圆环旋转一圈的时间-->
        <attr name="ringRotate_animTime" format="integer" />
    </declare-styleable>

    <!--二维码扫描控件-->
    <declare-styleable name="ViewMKQRScan">
        <!-- 扫描框的宽度，默认值为200dp -->
        <attr name="viewMKQRScan_rectWidth" format="dimension|reference" />
        <!-- 扫描边框的宽度，默认值为1dp -->
        <attr name="viewMKQRScan_borderSize" format="dimension|reference" />
        <!-- 扫描边框的颜色，默认值为@android:color/white -->
        <attr name="viewMKQRScan_borderColor" format="color|reference" />
        <!-- 扫描线的图片资源，默认值为null -->
        <attr name="viewMKQRScan_customScanLineDrawable" format="reference" />
        <!-- 扫描线从顶部移动到底部的动画时间，默认值为1000 -->
        <attr name="viewMKQRScan_animTime" format="integer" />
        <!-- 扫描线是否来回移动，默认值为true -->
        <attr name="viewMKQRScan_isScanLineReverse" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ViewMKSignature">
        <attr name="viewMKSignature_lineWidth" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="ViewMKSteps">
        <!--默认距离边缘的距离-->
        <attr name="viewMKSteps_margin" format="dimension|reference" />
        <!--标题向下偏移-->
        <attr name="viewMKSteps_titleMarginTop" format="dimension|reference" />
        <!--完成步骤的颜色-->
        <attr name="viewMKSteps_doneLineColor" format="color|reference" />
        <!--未完成的步骤的颜色-->
        <attr name="viewMKSteps_undoLineColor" format="color|reference" />
        <!--完成步骤的标题颜色-->
        <attr name="viewMKSteps_doneTitleColor" format="color|reference" />
        <!--未完成步骤的标题颜色-->
        <attr name="viewMKSteps_undoTitleColor" format="color|reference" />
        <!--完成步骤的数字颜色-->
        <attr name="viewMKSteps_doneNumberColor" format="color|reference" />
        <!--未完成步骤的数字颜色-->
        <attr name="viewMKSteps_undoNumberColor" format="color|reference" />
        <!--圆环的半径-->
        <attr name="viewMKSteps_circleRadius" format="dimension|reference" />
        <!--线的宽度-->
        <attr name="viewMKSteps_lineWidth" format="dimension|reference" />
        <!--步骤数字的大小-->
        <attr name="viewMKSteps_numberTextSize" format="dimension|reference" />
        <!--标题的大小-->
        <attr name="viewMKSteps_titleTextSize" format="dimension|reference" />
    </declare-styleable>
</resources>