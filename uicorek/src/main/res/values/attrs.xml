<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--btnk-->
    <declare-styleable name="BtnKIconFont">
        <attr name="btnKIcon_fontPath" format="string" />
    </declare-styleable>

    <!--viewk-->
    <declare-styleable name="ImageKMask">
        <!--蒙版类型-->
        <attr name="imageKMask_maskType" format="enum">
            <!--蒙版类型:全遮罩-->
            <enum name="stroke" value="1" />
            <!--蒙版类型:边框-->
            <enum name="fill" value="0" />
        </attr>
        <!--蒙版颜色-->
        <attr name="imageKMask_maskColor" format="color|reference" />
        <!--蒙版边框类型宽-->
        <attr name="imageKMask_borderWidth" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_search-->
    <attr name="LayoutKSearch_LayoutKSearchStyle" format="reference" />
    <declare-styleable name="LayoutKSearch">
        <attr name="layoutKSearch_search_icon" format="string|reference" />
        <attr name="layoutKSearch_search_iconSize" format="dimension|reference" />
        <attr name="layoutKSearch_search_iconPadding" format="dimension|reference" />
        <attr name="layoutKSearch_search_background" format="reference" />
        <attr name="layoutKSearch_search_textSize" format="dimension|reference" />
        <attr name="layoutKSearch_search_textColor" format="color|reference" />

        <attr name="layoutKSearch_clear_icon" format="string|reference" />
        <attr name="layoutKSearch_clear_iconSize" format="dimension|reference" />

        <attr name="layoutKSearch_hint_text" format="string|reference" />
        <attr name="layoutKSearch_hint_textSize" format="dimension|reference" />
        <attr name="layoutKSearch_hint_textColor" format="color|reference" />
        <attr name="layoutKSearch_hint_gravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
        </attr>

        <attr name="layoutKSearch_keyword_icon" format="string|reference" />
        <attr name="layoutKSearch_keyword_iconSize" format="dimension|reference" />
        <attr name="layoutKSearch_keyword_iconColor" format="color|reference" />
        <attr name="layoutKSearch_keyword_background" format="reference" />
        <attr name="layoutKSearch_keyword_maxLength" format="integer|reference" />
        <attr name="layoutKSearch_keyword_padding" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_navbar-->
    <attr name="LayoutKNavBar_LayoutKNavBarStyle" format="reference" />
    <declare-styleable name="LayoutKNavBar">
        <attr name="layoutKNavBar_title" format="string|reference" />
        <attr name="layoutKNavBar_title_textColor" format="color|reference" />
        <attr name="layoutKNavBar_title_textSize" format="dimension|reference" />

        <attr name="layoutKNavBar_subTitle" format="string|reference" />
        <attr name="layoutKNavBar_subTitle_textSize" format="dimension|reference" />
        <attr name="layoutKNavBar_subTitle_textColor" format="color|reference" />
        <attr name="layoutKNavBar_subTitle_marginTop" format="dimension|reference" />

        <attr name="layoutKNavBar_lineColor" format="color|reference" />
        <attr name="layoutKNavBar_lineWidth" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_banner-->
    <declare-styleable name="LayoutKBanner">
        <attr name="layoutKBanner_autoPlay" format="boolean" />
        <attr name="layoutKBanner_loop" format="boolean" />
        <attr name="layoutKBanner_scrollDuration" format="integer" />
        <attr name="layoutKBanner_intervalTime" format="integer" />
    </declare-styleable>

    <!--layoutk_slider-->
    <declare-styleable name="LayoutKSlider">
        <attr name="layoutKSlider_sliderHeight" format="reference|dimension" />
        <attr name="layoutKSlider_sliderRodLeftColor" format="color|reference" />
        <attr name="layoutKSlider_sliderRodLeftGradientColor" format="color|reference" />
        <attr name="layoutKSlider_sliderRodRightColor" format="color|reference" />
        <attr name="layoutKSlider_rodColor" format="color|reference" />
        <attr name="layoutKSlider_rodColorInside" format="reference|color" />
        <attr name="layoutKSlider_rodIsInside" format="boolean" />
        <attr name="layoutKSlider_rodRadius" format="dimension|reference" />
        <attr name="layoutKSlider_rodRadiusInside" format="dimension|reference" />
        <attr name="layoutKSlider_rodMinVal" format="float" />
        <attr name="layoutKSlider_rodMaxVal" format="float" />
    </declare-styleable>

    <declare-styleable name="LayoutKEmpty">
        <attr name="layoutKEmpty_icon" format="string|reference" />
        <attr name="layoutKEmpty_image" format="reference" />
        <attr name="layoutKEmpty_title" format="string|reference" />
        <attr name="layoutKEmpty_content" format="string|reference" />
        <attr name="layoutKEmpty_helpIcon" format="string|reference" />
        <attr name="layoutKEmpty_buttonTitle" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKAmount">
        <attr name="layoutKAmount_minVal" format="integer" />
        <attr name="layoutKAmount_maxVal" format="integer" />
        <attr name="layoutKAmount_defaultAmount" format="integer" />

        <attr name="layoutKAmount_amountTextColor" format="color|reference" />
        <attr name="layoutKAmount_amountTextSize" format="dimension|reference" />
        <attr name="layoutKAmount_amountBackgroundColor" format="color|reference" />
        <attr name="layoutKAmount_amountMarginHorizontal" format="dimension|reference" />
        <attr name="layoutKAmount_amountMinWidth" format="dimension|reference" />

        <attr name="layoutKAmount_btnTextColor" format="color|reference" />
        <attr name="layoutKAmount_btnTextSize" format="dimension|reference" />
        <attr name="layoutKAmount_btnBackgroundColor" format="color|reference" />
        <attr name="layoutKAmount_btnSize" format="dimension|reference" />
    </declare-styleable>

    <!--视频布局-->
    <declare-styleable name="LayoutKVideo">
        <!--视频地址-->
        <attr name="layoutKVideo_pathOrUrl" format="string|reference" />
        <!--视频是否循环播放-->
        <attr name="layoutKVideo_isLoop" format="boolean" />
        <!--视频位置权重-->
        <attr name="layoutKVideo_videoGravity" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
            <enum name="centerCrop" value="2" />
            <enum name="none" value="3" />
        </attr>
        <!--音量0-10-->
        <attr name="layoutKVideo_volume" format="integer" />
    </declare-styleable>

    <!--毛玻璃布局-->
    <declare-styleable name="LayoutKBlurred">
        <!--图片src-->
        <attr name="layoutKBlurred_src" format="reference" />
        <!--是否图片移动-->
        <attr name="layoutKBlurred_isMove" format="boolean" />
        <!--是否禁用模糊-->
        <attr name="layoutKBlurred_isBlurredEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LayoutKInputItem">
        <attr name="layoutKInputItem_hint" format="string|reference" />
        <attr name="layoutKInputItem_title" format="string|reference" />
        <attr name="layoutKInputItem_inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="number" value="2" />
        </attr>
        <attr name="layoutKInputItem_inputAppearance" format="reference" />
        <attr name="layoutKInputItem_titleAppearance" format="reference" />
        <attr name="layoutKInputItem_topLineAppearance" format="reference" />
        <attr name="layoutKInputItem_bottomLineAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKInputItem_TitleAppearance">
        <attr name="titleAppearance_titleColor" format="reference|color" />
        <attr name="titleAppearance_titleSize" format="dimension|reference" />
        <attr name="titleAppearance_leftPadding" format="dimension|reference" />
        <attr name="titleAppearance_minWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LayoutKInputItem_InputAppearance">
        <attr name="inputAppearance_hintColor" format="reference|color" />
        <attr name="inputAppearance_inputColor" format="color|reference" />
        <attr name="inputAppearance_textSize" format="dimension|reference" />
        <attr name="inputAppearance_maxInputLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="LayoutKInputItem_LineAppearance">
        <attr name="lineAppearance_color" format="reference|color" />
        <attr name="lineAppearance_height" format="dimension|reference" />
        <attr name="lineAppearance_leftMargin" format="dimension|reference" />
        <attr name="lineAppearance_rightMargin" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_side-->
    <declare-styleable name="LayoutKSideList">
        <attr name="layoutKSideList_menuWidth" format="dimension|reference" />
        <attr name="layoutKSideList_menuHeight" format="dimension|reference" />
        <attr name="layoutKSideList_menuItemTextSize" format="dimension|reference" />
        <attr name="layoutKSideList_menuItemTextSizeSelect" format="dimension|reference" />
        <attr name="layoutKSideList_menuItemTextColor" format="reference|color" />
        <attr name="layoutKSideList_menuItemTextColorSelect" format="reference|color" />
        <attr name="layoutKSideList_menuItemBackgroundColor" format="reference|color" />
        <attr name="layoutKSideList_menuItemBackgroundColorSelect" format="reference|color" />
        <attr name="layoutKSideList_menuItemIndicator" format="reference" />
        <attr name="layoutKSideList_subTextSize" format="dimension|reference" />
        <attr name="layoutKSideList_subTextColor" format="color|reference" />
        <attr name="layoutKSideList_subHeight" format="dimension|reference" />
        <attr name="layoutKSideList_subMarginStart" format="dimension|reference" />
        <attr name="layoutKSideList_contentTextSize" format="dimension|reference" />
        <attr name="layoutKSideList_contentTextColor" format="color|reference" />
        <attr name="layoutKSideList_contentImgRatio" format="enum">
            <enum name="ratio11" value="0" />
            <enum name="ratio34" value="1" />
            <enum name="ratio169" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LayoutKBtnSwitchApple">
        <!--默认开关状态-->
        <attr name="layoutKBtnSwitchApple_defaultStatus" format="boolean" />
        <!--打开时颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorOn" format="reference|color" />
        <!--关闭时颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorOff" format="reference|color" />
        <!--外边框颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorBorder" format="reference|color" />
        <!--中心圆颜色-->
        <attr name="layoutKBtnSwitchApple_iconColorBtn" format="reference|color" />
        <!--外边框厚度-->
        <attr name="layoutKBtnSwitchApple_borderWidth" format="dimension|reference" />
        <!--变换时间-->
        <attr name="layoutKBtnSwitchApple_animTime" format="integer" />
        <!--中心圆到外边距的距离-->
        <attr name="layoutKBtnSwitchApple_iconMargin" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKBtnIcon">
        <attr name="layoutKBtnIcon_background" format="reference" />
        <attr name="layoutKBtnIcon_iconSrc" format="reference" />
        <attr name="layoutKBtnIcon_iconSize" format="dimension|reference" />
        <attr name="layoutKBtnIcon_iconMarginRight" format="dimension|reference" />
        <attr name="layoutKBtnIcon_text" format="string|reference" />
        <attr name="layoutKBtnIcon_textColor" format="color|reference" />
        <attr name="layoutKBtnIcon_textSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKSpinnerForm">
        <attr name="layoutKSpinnerForm_isRequired" format="boolean" />
        <attr name="layoutKSpinnerForm_requiredIconPos" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="layoutKSpinnerForm_label" format="string|reference" />
        <attr name="layoutKSpinnerForm_labelTextSize" format="dimension|reference" />
        <attr name="layoutKSpinnerForm_labelColor" format="color|reference" />
        <attr name="layoutKSpinnerForm_labelMarginLeft" format="dimension|reference" />
        <attr name="layoutKSpinnerForm_labelMarginRight" format="dimension|reference" />
        <attr name="layoutKSpinnerForm_labelWidth" format="dimension|reference" />
        <!--StringArray 字符串数组-->
        <attr name="layoutKSpinnerForm_spinnerEntries" format="reference" />
        <!--layout item布局-->
        <attr name="layoutKSpinnerForm_spinnerItemLayout" format="reference" />
        <attr name="layoutKSpinnerForm_borderBackground" format="reference" />
    </declare-styleable>

    <!--textk-->
    <declare-styleable name="TextKBubble">
        <!--设置箭头朝向-->
        <attr name="textKBubble_arrowDirection" format="enum">
            <enum name="none" value="-1" />
            <enum name="auto" value="0" />
            <enum name="left" value="1" />
            <enum name="up" value="2" />
            <enum name="right" value="3" />
            <enum name="down" value="4" />
        </attr>
        <!--设置箭头三角形厚度-->
        <attr name="textKBubble_arrowHeight" format="dimension|reference" />
        <!--设置箭头三角形底宽-->
        <attr name="textKBubble_arrowWidth" format="dimension|reference" />
        <!--设置箭头在边线上的位置策略-->
        <attr name="textKBubble_arrowPosPolicy" format="enum">
            <!--箭头指向目标View的中心点-->
            <enum name="target_center" value="0" />
            <!--箭头从自己的中心点发出-->
            <enum name="self_center" value="1" />
            <!--结合setArrowPosOffset，箭头从所在轴的头端开始偏移-->
            <enum name="self_begin" value="2" />
            <!--结合setArrowPosOffset，箭头从所在轴的尾端开始偏移-->
            <enum name="self_end" value="3" />
        </attr>
        <!--设置箭头在所在边线上的偏移距离-->
        <attr name="textKBubble_arrowPosOffset" format="dimension|reference" />
        <!--设置箭头指向的View对象Id-->
        <attr name="textKBubble_arrowToViewId" format="reference" />
        <!--设置气泡背景色-->
        <attr name="textKBubble_bgColor" format="color|reference" />
        <!--设置边框线颜色-->
        <attr name="textKBubble_borderColor" format="color|reference" />
        <!--设置边框线宽-->
        <attr name="textKBubble_borderWidth" format="dimension|reference" />
        <!--设置边框于背景之间的间隙宽度-->
        <attr name="textKBubble_gapPadding" format="dimension|reference" />

        <!--设置边角弧度-->
        <attr name="textKBubble_cornerRadius" format="dimension|reference" />
        <!--设置边角弧度左上角-->
        <attr name="textKBubble_cornerTopLeftRadius" format="dimension|reference" />
        <!--设置边角弧度右上角-->
        <attr name="textKBubble_cornerTopRightRadius" format="dimension|reference" />
        <!--设置边角弧度左下角-->
        <attr name="textKBubble_cornerBottomLeftRadius" format="dimension|reference" />
        <!--设置边角弧度右下角-->
        <attr name="textKBubble_cornerBottomRightRadius" format="dimension|reference" />

        <!--设置内边距-->
        <attr name="textKBubble_padding" format="dimension|reference" />
        <!--设置左右内边距-->
        <attr name="textKBubble_paddingHorizontal" format="dimension|reference" />
        <!--设置上下内边距-->
        <attr name="textKBubble_paddingVertical" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="TextKIconFont">
        <attr name="textKIconFont_fontPath" format="string" />
    </declare-styleable>

    <!--下划线控件-->
    <declare-styleable name="TextKUnderLine">
        <!--下划线是否出现-->
        <attr name="textKUnderLine_underline" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextKForm">
        <attr name="textKForm_isRequired" format="boolean" />
        <attr name="textKForm_requiredIconPos" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="textKForm_label" format="string|reference" />
        <attr name="textKForm_labelTextSize" format="dimension|reference" />
        <attr name="textKForm_labelColor" format="color|reference" />
        <attr name="textKForm_labelMarginLeft" format="dimension|reference" />
        <attr name="textKForm_labelMarginRight" format="dimension|reference" />
        <attr name="textKForm_labelWidth" format="dimension|reference" />
        <attr name="textKForm_textColor" format="color|reference" />
        <attr name="textKForm_textHint" format="string|reference" />
        <attr name="textKForm_textSize" format="dimension|reference" />
        <attr name="textKForm_borderBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="TextKEditForm">
        <attr name="textKEditForm_isRequired" format="boolean" />
        <attr name="textKEditForm_requiredIconPos" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="textKEditForm_label" format="string|reference" />
        <attr name="textKEditForm_labelTextSize" format="dimension|reference" />
        <attr name="textKEditForm_labelColor" format="color|reference" />
        <attr name="textKEditForm_labelMarginLeft" format="dimension|reference" />
        <attr name="textKEditForm_labelMarginRight" format="dimension|reference" />
        <attr name="textKEditForm_labelWidth" format="dimension|reference" />
        <attr name="textKEditForm_editHint" format="string|reference" />
        <attr name="textKEditForm_editType" format="enum">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
            <enum name="textPassword" value="2" />
            <enum name="numberPassword" value="3" />
        </attr>
        <attr name="textKEditForm_editColor" format="color|reference" />
        <attr name="textKEditForm_editSize" format="dimension|reference" />
        <attr name="textKEditForm_borderBackground" format="reference" />
        <attr name="textKEditForm_borderBackgroundHasFocus" format="reference" />
    </declare-styleable>

    <!--viewk-->
    <declare-styleable name="CircleSpread">
        <!--中心圆颜色-->
        <attr name="circleSpread_centerColor" format="reference|color" />
        <!--中心圆图片-->
        <attr name="circleSpread_centerDrawable" format="reference" />
        <!--中心圆半径-->
        <attr name="circleSpread_centerRadius" format="dimension|reference" />
        <!--扩散圆颜色-->
        <attr name="circleSpread_spreadColor" format="reference|color" />
        <!--扩散圆最大数-->
        <attr name="circleSpread_spreadCount" format="integer" />
        <!--扩散圆波浪叠层效果还是边缘波纹效果-->
        <attr name="circleSpread_spreadIsRipple" format="boolean" />
        <!--扩散间距-->
        <attr name="circleSpread_circlesDistance" format="dimension|reference" />
        <!--扩散时间1-1000ms之间-->
        <attr name="circleSpread_animTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="ArcRotate">
        <!-- 圆弧的开始渐变色-->
        <attr name="arcRotate_arcStartColor" format="color|reference" />
        <!-- 圆弧的结束渐变色-->
        <attr name="arcRotate_arcEndColor" format="color|reference" />
        <!-- 圆弧的线宽-->
        <attr name="arcRotate_arcWidth" format="dimension|reference" />
        <!-- 圆弧个数 1-11-->
        <attr name="arcRotate_arcNum" format="integer" />
        <!-- 圆弧之间的间隔 , 默认30°, 0-360-->
        <attr name="arcRotate_angleInterval" format="integer" />
        <!-- 圆弧步长-->
        <attr name="arcRotate_angleStep" format="integer" />
        <!-- 圆弧走一轮所需的时间-->
        <attr name="arcRotate_animTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="ViewKColorPicker">
        <attr name="viewKColorPicker_colorStart" format="color|reference" />
        <attr name="viewKColorPicker_colorEnd" format="color|reference" />
    </declare-styleable>

    <!--二维码扫描控件-->
    <declare-styleable name="ViewKQRScan">
        <!-- 扫描边框线的宽度，默认值为1dp -->
        <attr name="viewKQRScan_borderLineWidth" format="dimension|reference" />
        <!-- 扫描边框的颜色，默认值为@android:color/white -->
        <attr name="viewKQRScan_borderColor" format="color|reference" />
        <!-- 扫描线的图片资源，默认值为null -->
        <attr name="viewKQRScan_lineDrawable" format="reference" />
        <!-- 扫描线的线宽，默认值为1dp -->
        <attr name="viewKQRScan_lineWidth" format="dimension|reference" />
        <!-- 扫描线从顶部移动到底部的动画时间，默认值为1000 -->
        <attr name="viewKQRScan_animTime" format="integer" />
        <!-- 扫描线是否来回移动，默认值为true -->
        <attr name="viewKQRScan_isScanLineReverse" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ViewKQRScan2">
        <!-- 扫描边框的粗细-->
        <attr name="viewKQRScan2_borderWidth" format="dimension|reference" />
        <!-- 扫描边框的颜色-->
        <attr name="viewKQRScan2_borderColor" format="color|reference" />
        <!-- 扫描线的图片资源-->
        <attr name="viewKQRScan2_lineDrawable" format="reference" />
        <!-- 扫描线的粗细-->
        <attr name="viewKQRScan2_lineWidth" format="dimension|reference" />
        <!-- 扫描线的颜色-->
        <attr name="viewKQRScan2_lineColor" format="color|reference" />
        <!-- 扫描线每次移动的距离(包括扫描线的移动,收缩和成功logo的展现)-->
        <attr name="viewKQRScan2_distance" format="dimension|reference" />
        <!-- 扫描线从顶部移动至底部的动画时间ms-->
        <attr name="viewKQRScan2_animTime" format="integer" />
        <!-- 扫描线是否来回移动-->
        <attr name="viewKQRScan2_isLineReverse" format="boolean" />
        <!-- 扫描成功的成功图标-->
        <attr name="viewKQRScan2_successDrawable" format="reference" />
        <!-- 扫描成功的成功图标大小-->
        <attr name="viewKQRScan2_successDrawableSize" format="dimension|reference" />
        <!-- 扫描成功的成功动画时间-->
        <attr name="viewKQRScan2_successAnimTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="RadarRipple">
        <!-- 扫描雷达背景颜色-->
        <attr name="radarRipple_bgColor" format="color|reference" />
        <!-- 扫描雷达颜色-->
        <attr name="radarRipple_radarColor" format="reference|color" />
        <!-- 扫描雷达线颜色-->
        <attr name="radarRipple_lineColor" format="reference|color" />
        <!-- 扫描雷达线的Alpha-->
        <attr name="radarRipple_lineAlpha" format="integer" />
        <!-- 扫描雷达线宽-->
        <attr name="radarRipple_lineWidth" format="dimension|reference" />
        <!-- 扫描雷达线数-->
        <attr name="radarRipple_lineCount" format="integer" />
        <!-- 扫描雷达扫描角度步长(1-360)-->
        <attr name="radarRipple_angleStep" format="integer" />
        <!-- 扫描雷达扫描一圈的时间ms-->
        <attr name="radarRipple_animTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="RadarWave">
        <!-- 扫描雷达颜色-->
        <attr name="radarWave_radarColor" format="reference|color" />
        <!-- 扫描雷达波浪颜色-->
        <attr name="radarWave_waveColor" format="reference|color" />
        <!-- 扫描雷达波浪层级渐变反转-->
        <attr name="radarWave_isWaveOverTurn" format="boolean" />
        <!-- 扫描雷达波浪层数-->
        <attr name="radarWave_waveCount" format="integer" />
        <!-- 扫描雷达扫描角度步长(1-360)-->
        <attr name="radarWave_angleStep" format="integer" />
        <!-- 扫描雷达扫描一圈的时间ms-->
        <attr name="radarWave_animTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="ViewKSignature">
        <attr name="viewKSignature_lineWidth" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="ViewKSteps">
