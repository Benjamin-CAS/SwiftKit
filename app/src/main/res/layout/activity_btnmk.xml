<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        tools:context=".btnmk.BtnMKActivity">

        <com.mozhimen.uicoremk.btnmk.SwitchApple
            android:id="@+id/btnmk_switchApple"
            android:layout_width="80dp"
            android:layout_height="45dp"
            app:switchApple_animTime="300"
            app:switchApple_bgColorBorder="@color/blue_light"
            app:switchApple_bgColorOff="@color/blue_light"
            app:switchApple_bgColorOn="@color/blue_normal"
            app:switchApple_borderWidth="1dp"
            app:switchApple_defaultStatus="false"
            app:switchApple_iconColorBtn="@color/white"
            app:switchApple_iconMargin="1dp" />
    </LinearLayout>
</layout>