<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mozhimen.basicktest">

    <application
        android:name=".BasicKApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SwiftKit">
        <activity
            android:name="com.mozhimen.basicktest.utilk.UtilKFileActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.basicktest.basek.BaseKDemoServiceActivity"
            android:exported="false" />
        <activity
            android:name=".loadk.LoadKActivity"
            android:exported="false" />
        <activity
            android:name=".BasicKActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mozhimen.basicktest.utilk.UtilKAnimActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.basicktest.utilk.UtilKGestureActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.basicktest.eventk.EventKActivity"
            android:exported="true" />
        <activity
            android:name="com.mozhimen.basicktest.utilk.UtilKActivity"
            android:exported="true" />
        <activity
            android:name="com.mozhimen.basicktest.executork.ExecutorKActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.basicktest.basek.BaseKDemoActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.basicktest.stackk.StackKActivity"
            android:exported="false" />

        <receiver
            android:name=".loadk.AutoRunReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter android:priority="1000">
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service android:name="com.mozhimen.basicktest.basek.BaseKDemoService" />
    </application>

</manifest>