<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".abilityk.opencvk.OpenCVKActivity">

        <com.google.android.material.button.MaterialButton
            style="@style/BtnStyle"
            android:onClick="goOpenCVKContrast"
            android:text="OpenCVKContrast"
            tools:ignore="HardcodedText,UsingOnClickInXml" />

        <com.google.android.material.button.MaterialButton
            style="@style/BtnStyle"
            android:onClick="goOpenCVKShape"
            android:text="OpenCVKShape"
            tools:ignore="HardcodedText,UsingOnClickInXml" />

        <com.google.android.material.button.MaterialButton
            style="@style/BtnStyle"
            android:onClick="goOpenCVKMatch"
            android:text="OpenCVKMatch"
            tools:ignore="HardcodedText,UsingOnClickInXml" />
    </LinearLayout>
</layout>